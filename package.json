{
  "name": "hardhat-project",
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^4.0.0",
    "@typechain/ethers-v6": "^0.5.1",
    "@typechain/hardhat": "^9.1.0",
    "concurrently": "^8.2.2",
    "hardhat": "^2.19.4",
    "typechain": "^8.3.2"
  },
  "scripts": {
    "blockchain": "dotenv -f .env run -- bash -c 'npx hardhat node --fork https://eth-mainnet.alchemyapi.io/v2/$ALCHEMY_API_KEY'",
    "build": "hardhat typechain",
    "terminal": "npx hardhat console --network localhost",
    "main": "npx hardhat run --network localhost src/main.ts",
    "start": "concurrently --kill-others --prefix-colors \"bgRed.bold,bgGreen.bold\" \"npm run blockchain\" \"sleep 10 && npm run main\""
  },
  "dependencies": {
    "@aave/core-v3": "^1.18.0",
    "@aave/periphery-v3": "^2.5.2",
    "@aave/protocol-v2": "^1.0.1",
    "@bgd-labs/aave-address-book": "^2.26.1",
    "@mycrypto/eth-scan": "^3.5.3",
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.6",
    "@nomicfoundation/hardhat-ethers": "^3.0.5",
    "@openzeppelin/contracts": "^5.0.2",
    "@types/node": "^20.11.20",
    "@uniswap/sdk": "^3.0.3",
    "@uniswap/universal-router": "^1.6.0",
    "@uniswap/v2-core": "^1.0.1",
    "@uniswap/v2-periphery": "^1.1.0-beta.0",
    "@uniswap/v3-core": "^1.0.1",
    "@uniswap/v3-periphery": "^1.4.4",
    "@uniswap/v3-sdk": "^3.11.0",
    "alchemy-sdk": "^3.2.0",
    "dotenv": "^16.3.1",
    "ethers": "^6.11.1",
    "hardhat-ethers": "^1.0.1",
    "web3": "^4.5.0"
  }
}
